            case 2:
                return ("Tuesday")
                break;
            case 3:
                return ("Wednesday")
                break;
            case 4:
                return ("Thursday")
                break;
            case 5:
                return ("Friday")
                break;
            case 6:
                return ("Saturday")
                break;
        }
    }
    
    function convert_hour(d){
            var h = addZero(d.getUTCHours());
            var m = addZero(d.getUTCMinutes());
            var s = addZero(d.getUTCSeconds());
            if (h >= 12) {
                if ( h != 12) {
                    h = h - 12;    
                }
                
                i = "PM"
            } else {
                if (h == 0) {
                    h = 12
                }
                i = "AM"
            }
            
            return h+":"+m+" "+i;
        }
        
        
        
    function addZero(i) {
        if (i < 10) {
            i = "0" + i;
        }
        return i;
    }
    
    
    function check_email (){
        var email = $("#fieldEmail").val();
        if( !validateEmail(email)) {
            return false
        } else {
            return true
        }
    }
    
    function validateEmail($email) {
      var emailReg = /^([\w-\.]+@([\w-]+\.)+[\w-]{2,4})?$/;
      if( !emailReg.test( $email ) ) {
        return false;
      } else {
        return true;
      }
    }
        

     $(document).ready(function() {
        $(".st_sharethis").mouseenter(function () {
            $(this).data('timeout', setTimeout(function () {
                send_ga_event('The Bay Centre – Hyper Local 2016 - Share')
            }, 500));
        }).mouseleave(function () {
            clearTimeout($(this).data('timeout'));
        });
        function renderAll(){
            var propertyDetails = getPropertyDetails();
            // renderPageData("#logo_container","#logo_template", propertyDetails, "logo")
            var today_hours = getTodaysHours()
            renderPageData("#find_left_div", "#find_left_template", propertyDetails, "footer_logo")
            renderPageData("#today_hour_container", "#today_hour_template", today_hours, "hours")
        }
        
        
        $('#footer_map').addClass('scrolloff'); // set the pointer events to none on doc ready
        $('#canvas1').on('click', function () {
            $('#footer_map').removeClass('scrolloff'); // set the pointer events true on click
        });

        // you want to disable pointer events when the mouse leave the canvas area;

        $("#footer_map").mouseleave(function () {
            $('#footer_map').addClass('scrolloff'); // set the pointer events to none when mouse leaves the map area
        });
        
        function renderPageData(container, template, collection, type){
            var item_list = [];
            var item_rendered = [];
            var template_html = $(template).html();
            Mustache.parse(template_html);   // optional, speeds up future uses
 
            item_list.push(collection);    
            
            
            $.each( item_list , function( key, val ) {
                if (type == "footer_logo" || type == "logo"){
                    if (val.contact_phone != ""){
                        val.phone_string = "P " + val.contact_phone
                    }    
                }
                
                if (type == "hours"){
                    val.day = get_day(val.day_of_week)
                    
                    if (val.open_time && val.close_time && (val.is_closed == false || val.is_closed == null)){
                        var open_time = new Date (val.open_time)
                        var close_time = new Date (val.close_time)
                        val.open_time = convert_hour(open_time);
                        val.close_time = convert_hour(close_time);    
                        val.h = val.open_time+ " - " + val.close_time;
                    } else {
                        val.h = "Closed"
                    }
                    
                }
                
                var rendered = Mustache.render(template_html,val);
                item_rendered.push(rendered);
            });
            
            $(container).show();
            $(container).html(item_rendered.join(''));

        }
        
        $('input').iCheck({
            checkboxClass: 'icheckbox_square-grey',
            radioClass: 'iradio_square',
            increaseArea: '20%' // optional
          });
        
        loadMallDataCached(renderAll);  
        
     });
</script>

